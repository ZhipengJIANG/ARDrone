package lib.ardrone.navigationdata.structures;

public class NavDataVisionRaw {

    private float vision_tx_raw, vision_ty_raw, vision_tz_raw;

    public void setVision_tz_raw(float vision_tz_raw) {
        this.vision_tz_raw = vision_tz_raw;
    }

    public float getVision_tz_raw() {
        return vision_tz_raw;
    }

    public void setVision_tx_raw(float vision_tx_raw) {
        this.vision_tx_raw = vision_tx_raw;
    }

    public float getVision_tx_raw() {
        return vision_tx_raw;
    }

    public void setVision_ty_raw(float vision_ty_raw) {
        this.vision_ty_raw = vision_ty_raw;
    }

    public float getVision_ty_raw() {
        return vision_ty_raw;
    }

    @ Override
    public String toString() {
        return "VISION_RAW: " + this.vision_tx_raw + " " + this.vision_ty_raw + " " + this.vision_tz_raw;
    }
}
