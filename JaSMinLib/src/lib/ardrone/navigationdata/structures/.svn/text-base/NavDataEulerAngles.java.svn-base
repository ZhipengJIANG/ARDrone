package lib.ardrone.navigationdata.structures;

public class NavDataEulerAngles {

    private float theta_a, phi_a;

    public void setTheta_a(float theta_a) {
        this.theta_a = theta_a;
    }

    public float getTheta_a() {
        return theta_a;
    }

    public void setPhi_a(float phi_a) {
        this.phi_a = phi_a;
    }

    public float getPhi_a() {
        return phi_a;
    }

    @ Override
    public String toString() {
        return "EULER_ANGLES: " + "THETA :" + this.theta_a + " PHI :" + this.phi_a;
    }
}
