package lib.ardrone.navigationdata.structures;

public class NavDataDemo {

    private int controlState = 0;
    private float pitch = 0;
    private float roll = 0;
    private float yaw = 0;
    private int altitude = 0;
    private int batteryPercentage = 0;
    private float vx = 0;
    private float vy = 0;
    private float vz = 0;

    public int getControlState() {
        return controlState;
    }

    public void setControlState(int state) {
        this.controlState = state;
    }

    public float getPitch() {
        return pitch;
    }

    public void setPitch(float pitch) {
        this.pitch = pitch;
    }

    public float getRoll() {
        return roll;
    }

    public void setRoll(float roll) {
        this.roll = roll;
    }

    public float getYaw() {
        return yaw;
    }

    public void setYaw(float yaw) {
        this.yaw = yaw;
    }

    public int getAltitude() {
        return altitude;
    }

    public void setAltitude(int altitude) {
        this.altitude = altitude;
    }

    public int getBatteryPercentage() {
        return batteryPercentage;
    }

    public void setBatteryPercentage(int bat) {
        this.batteryPercentage = bat;
    }

    public float getVelocityX() {
        return vx;
    }

    public void setVelocityX(float vx) {
        this.vx = vx;
    }

    public float getVelocityY() {
        return vy;
    }

    public void setVelocityY(float vy) {
        this.vy = vy;
    }

    public float getVelocityZ() {
        return vz;
    }

    public void setVelocityZ(float vz) {
        this.vz = vz;
    }

    @ Override
    public String toString() {
        return "NAVDATA_DEMO: " + "STATE :" + this.controlState + " PITCH :" + this.pitch + " ROLL :" + this.roll + " YAW :" + this.yaw + " VELOCITY :" + this.vx + " " + this.vy + " " + this.vz + " ALTITUDE :" + this.altitude + " BATTERY :" + this.batteryPercentage + "%";
    }
}
