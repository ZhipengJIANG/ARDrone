package lib.ardrone.navigationdata.structures;

public class NavDataPwm {

    private int motor1, motor2, motor3, motor4;
    private int gaz_feed_forward, gaz_altitude;
    private float altitude_integral, vz_ref;
    private int u_pitch, u_roll, u_yaw;
    private float yaw_u_I;
    private int u_pitch_planif, u_roll_planif, u_yaw_planif;
    private float yaw_u_I_planif;

    public void setMotor1(int motor1) {
        this.motor1 = motor1;
    }

    public int getMotor1() {
        return motor1;
    }

    public void setMotor4(int motor4) {
        this.motor4 = motor4;
    }

    public int getMotor4() {
        return motor4;
    }

    public void setMotor2(int motor2) {
        this.motor2 = motor2;
    }

    public int getMotor2() {
        return motor2;
    }

    public void setMotor3(int motor3) {
        this.motor3 = motor3;
    }

    public int getMotor3() {
        return motor3;
    }

    public void setGaz_altitude(int gaz_altitude) {
        this.gaz_altitude = gaz_altitude;
    }

    public int getGaz_altitude() {
        return gaz_altitude;
    }

    public void setGaz_feed_forward(int gaz_feed_forward) {
        this.gaz_feed_forward = gaz_feed_forward;
    }

    public int getGaz_feed_forward() {
        return gaz_feed_forward;
    }

    public void setAltitude_integral(float altitude_integral) {
        this.altitude_integral = altitude_integral;
    }

    public float getAltitude_integral() {
        return altitude_integral;
    }

    public void setVz_ref(float vz_ref) {
        this.vz_ref = vz_ref;
    }

    public float getVz_ref() {
        return vz_ref;
    }

    public void setU_roll(int u_roll) {
        this.u_roll = u_roll;
    }

    public int getU_roll() {
        return u_roll;
    }

    public void setU_pitch(int u_pitch) {
        this.u_pitch = u_pitch;
    }

    public int getU_pitch() {
        return u_pitch;
    }

    public void setU_yaw(int u_yaw) {
        this.u_yaw = u_yaw;
    }

    public int getU_yaw() {
        return u_yaw;
    }

    public void setYaw_u_I(float yaw_u_I) {
        this.yaw_u_I = yaw_u_I;
    }

    public float getYaw_u_I() {
        return yaw_u_I;
    }

    public void setU_roll_planif(int u_roll_planif) {
        this.u_roll_planif = u_roll_planif;
    }

    public int getU_roll_planif() {
        return u_roll_planif;
    }

    public void setU_yaw_planif(int u_yaw_planif) {
        this.u_yaw_planif = u_yaw_planif;
    }

    public int getU_yaw_planif() {
        return u_yaw_planif;
    }

    public void setU_pitch_planif(int u_pitch_planif) {
        this.u_pitch_planif = u_pitch_planif;
    }

    public int getU_pitch_planif() {
        return u_pitch_planif;
    }

    public void setYaw_u_I_planif(float yaw_u_I_planif) {
        this.yaw_u_I_planif = yaw_u_I_planif;
    }

    public float getYaw_u_I_planif() {
        return yaw_u_I_planif;
    }

    @Override
    public String toString() {
        return "PWM: " + "motors: " + this.motor1 + " " + this.motor2 + " " + this.motor3 + this.motor4 + " Gaz_feed_fwd: " + this.gaz_altitude + " Gaz_altitude: " + this.gaz_altitude + " Alti_integral: " + this.altitude_integral + " Vz_ref: " + this.vz_ref + " pitch: " + this.u_pitch + " roll: " + this.u_roll + " yaw: " + this.u_yaw;
    }
}
